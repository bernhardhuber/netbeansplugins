<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Tools">
            <file name="org-huberb-nbpmdrulesets-PMDDesignAction.instance"/>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Tools">
            <file name="org-huberb-nbpmdrulesets-PMDDesignAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Tools/org-huberb-nbpmdrulesets-PMDDesignAction.instance"/>
            </file>
            <attr name="org-huberb-nbpmdrulesets-PMDDesignAction.shadow/DerbyDatabaseAction.instance" boolvalue="true"/>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <folder name="Advanced">
            <file name="org-huberb-nbpmdrulesets-options-NbpmdrulesetsAdvancedOption.instance"/>
        </folder>
    </folder>
    <folder name="Plugins">
        <!-- Register XML module catalog plugin -->
        <folder name="XML">
            <folder name="UserCatalogs">
                <file name="org-huberb-nbpmdrulesets-catalog-RegisterCatalog.instance">
                    <attr name="instanceCreate" newvalue="org.huberb.nbpmdrulesets.catalog.RegisterCatalog"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.xml.catalog.spi.CatalogReader"/>
                </file>
            </folder>
        </folder>
    </folder>
    
    <folder name="Services">
        <file name="PmdRulesetSettings.settings" url="options/PmdRulesetSettings.xml">
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/huberb/nbpmdrulesets/resources/PMDOptionsSettingsIcon.gif"/>
        </file>
        <folder name="JavaHelp">
            <file name="nbpmdrulesets-helpset.xml" url="nbpmdrulesets-helpset.xml"/>
        </folder>
    </folder>
    
    <folder name="Templates">
        <folder name="PMD">
            <!--file name="pmdrulesetwzard">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.huberb.nbpmdrulesets.Bundle"/>
                <attr name="instantiatingIterator" newvalue="org.huberb.nbpmdrulesets.wizard.PMDRulesetWizardIterator"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/org/huberb/nbpmdrulesets/wizard/pMDRuleset.html"/>
            </file-->
            <file name="pmdrulesetsimple.xml" url="resources/pmd-ruleset-template-simple.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.huberb.nbpmdrulesets.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="instantiatingWizardURL" urlvalue="nbresloc:/org/huberb/nbpmdrulesets/resources/pmdRulesetSimple.html"/>
                
            </file>
        </folder>
    </folder>
    
    <folder name="PmdRulesets">
        <file name="pmd-ruleset-ant.xml" url="resources/pmd-ruleset-ant.xml"/>
    </folder>
</filesystem>
